# Prompt Manager 配置文件
# 如果不存在此文件，将使用默认配置

database:
  # 数据库类型: sqlite 或 mysql
  type: "sqlite"
  # SQLite 数据库文件名（不含 .db 后缀）
  # MySQL 数据库名
  name: "prompt_manager"
  # MySQL 配置（type=mysql 时生效）
  host: "localhost"
  port: 3306
  user: "root"
  password: ""

logging:
  # 日志级别: debug, info, warn, error
  level: "info"
  # 日志输出: stdout 或文件路径
  output: "stdout"
